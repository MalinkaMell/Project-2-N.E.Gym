<!-- Instructor -->
<div class="card mb-3">
  <div class="card-header d-flex justify-content-between align-items-center">
    <div>
      <i class="fas fa-table"></i>
      {{ title }}
    </div>
    <div id="save_btn_placeholder">
      {{#ifMore user.memberLvl 5}}<button class="btn btn-dark" id="edit_instructor">Edit</button>{{/ifMore}}
      {{#ifMore user.memberLvl 6}}<button class="btn btn-danger" id="delete_instructor">Delete</button>{{/ifMore}}
    </div>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" width="100%" cellspacing="0">
        <tr>
          <td>ID:</td>
          <td id="instructor_id">{{ results.id }}</td>
        </tr>
        <tr>
          <td>Name</td>
          <td><span id="instructor_f_name">{{ results.firstName }}</span> <span id="instructor_l_name">{{ results.lastName }}</span></td>
        </tr>
        <tr>
          <td>Bio</td>
          <td id="instructor_bio">{{ results.bio }}</td>
        </tr>
        <tr>
          <td>Photo</td>
          <td id="instructor_img_td"><img src="{{ results.photo }}" class="img-thumbnail" id="instructor_img_url" width="250">
          </td>
        </tr>
        <tr>
          <td>Email</td>
          <td id="instructor_email">{{ results.email }}</td>
        </tr>
         <tr>
          <td>Phone</td>
          <td id="instructor_phone">{{ results.phone }}</td>
        </tr>
      </table>
    </div>
  </div>
  <div class="card-footer small text-muted">Last updated <span id="date"></span></div>
</div>